{
    "manifest_version": 3,
    "name": "HitBullEye",
    "description": "This extension help to automate HitBullEye site.",
    "version": "2.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}
